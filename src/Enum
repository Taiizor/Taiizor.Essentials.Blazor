const Direction = {
	E: 'East',
	W: 'West',
	N: 'North',
	S: 'South',
	NE: 'North-East',
	NW: 'North-West',
	SE: 'South-East',
	SW: 'South-West'
}

let myDirection = 'South-East';

if (myDirection === Direction.SE) {
	console.log("Yes, your direction is South-East");
} else {
	console.log("No, your direction is not South-East");
}